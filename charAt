public class A {
public static void main(String args[])
{
String s="sanjay";
char a;
int count=0;
int n=s.length();
for (int i=0;i<n;i++)
{
	count=0;
	a=s.charAt(i);
	for (int j=0;j<n;j++)
	{
		if(s.charAt(j)==a)
		{
			count++;
		}
	}
	System.out.println("the count of " +a+ " is " + count );
}
}
}

